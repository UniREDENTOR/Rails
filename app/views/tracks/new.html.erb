<h3> New Track</h3>
<a href="/tracks"><button > Back </button></a>
<%= form_for @track do |f| %>
Name:         <%= f.text_field :name %>
Album:        <%= f.text_field :album_id %>
Media type:   <%= f.text_field :media_type_id %>
Genero:       <%= f.text_field :genre_id %>
Composer:     <%= f.text_field :composer %>
milliseconds: <%= f.text_field :milliseconds %>
Bytes:        <%= f.text_field :bytes %>
Unit Price:   <%= f.text_field :unit_price %>
              <%= f.submit 'Save' %>
<% end %>