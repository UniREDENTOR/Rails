<h3> New Track</h3>
<a href="../"><button > Back </button></a>
<%= form_for @track do |f| %>
Name:         <%= f.text_field :name %>
Album:        <%= f.number_field :album_id %>
Media type:   <%= f.number_field :media_type_id %>
Genero:       <%= f.number_field :genre_id %>
Composer:     <%= f.text_field :composer %>
milliseconds: <%= f.number_field :milliseconds %>
Bytes:        <%= f.number_field :bytes %>
Unit Price:   <%= f.number_field :unit_price %>
              <%= f.submit 'Save' %>
<% end %>